FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
       type            turbulentMixingLengthFrequencyInlet;
        mixingLength    1.2;
       value           $internalField;
    }

     wall-part-solid
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    turbine
    {
        type            omegaWallFunction;
        value           $internalField;

    }

   

}


