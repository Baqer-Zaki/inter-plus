FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 22.4;

boundaryField
{
    wall
    {
        type            omegaWallFunction;
        value           uniform 22.4;
    }
    topPatch
    {
        type            zeroGradient;
    }
    bottomPatch
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.0007;
        value           nonuniform List<scalar> 0();
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 22.4;
    }
}


