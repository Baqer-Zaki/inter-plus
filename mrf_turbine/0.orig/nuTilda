FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
       type            calculated;
       value           $internalField;
    }

    Walls
    {
       type            calculated;
       value           $internalField;
    }

    Outlet
    {
        type            calculated;
	 value           $internalField;
    }
   FrontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
