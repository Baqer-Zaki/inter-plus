FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5.39e-07;

boundaryField
{
    topPatch
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    wall
    {
        type            calculated;
        value           uniform 5.39e-07;
    }
    bottomPatch
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 5.39e-07;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 5.39e-07;
    }
}


